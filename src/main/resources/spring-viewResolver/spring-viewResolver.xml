<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.3.xsd"
	default-autowire="byName">
	
	<import resource="classpath*:spring-viewResolver/xml.xml"/>
	<import resource="classpath*:spring-viewResolver/beanName.xml"/>
	<import resource="classpath*:spring-viewResolver/internalResource.xml"/>
	<import resource="classpath*:spring-viewResolver/resourceBundle.xml"/>
	<import resource="classpath*:spring-viewResolver/urlBasedView.xml"/>
</beans>